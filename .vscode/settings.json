{
  "rust-analyzer.linkedProjects": ["./server/Cargo.toml"],
  "rust-analyzer.rustfmt.extraArgs": ["+nightly"],
  "rust-analyzer.check.command": "clippy",
  "[rust]": {
    "editor.rulers": [120]
  },
  "cSpell.words": [
    "aarch",
    "canonicalize",
    "clientrc",
    "Commentable",
    "formated",
    "Hasher",
    "outfile",
    "slicec",
    "typeref",
    "vsix"
  ],
  "slice.configurations": [
    {
      "paths": ["slice"],
      "addWellKnownTypes": false
    }
  ]
}
